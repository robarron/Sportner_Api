{% extends 'base.html.twig' %}

{% block title %}SponsorshipCode index{% endblock %}

{% block body %}
    <h1>SponsorshipCode index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>SponsorshipCode</th>
                <th>Sponsorshipchecked</th>
                <th>ChildNumber</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for sponsorship_code in sponsorship_codes %}
            <tr>
                <td>{{ sponsorship_code.id }}</td>
                <td>{{ sponsorship_code.sponsorshipCode }}</td>
                <td>{{ sponsorship_code.sponsorshipchecked ? 'Yes' : 'No' }}</td>
                <td>{{ sponsorship_code.childNumber }}</td>
                <td>
                    <a href="{{ path('sponsorship_code_show', {'id': sponsorship_code.id}) }}">show</a>
                    <a href="{{ path('sponsorship_code_edit', {'id': sponsorship_code.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="5">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('sponsorship_code_new') }}">Create new</a>
{% endblock %}
